<?xml version='1.0' encoding='UTF-8'?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.scala-lang</groupId>
    <artifactId>scala3-compiler_3</artifactId>
    <packaging>jar</packaging>
    <description>scala3-compiler-bootstrapped</description>
    <url>https://scala-lang.org/</url>
    <version>3.8.0</version>
    <licenses>
        <license>
            <name>Apache-2.0</name>
            <url>https://www.apache.org/licenses/LICENSE-2.0</url>
            <distribution>repo</distribution>
        </license>
    </licenses>
    <name>scala3-compiler-bootstrapped</name>
    <organization>
        <name>LAMP/EPFL</name>
        <url>http://lamp.epfl.ch</url>
    </organization>
    <scm>
        <url>https://github.com/scala/scala3</url>
        <connection>scm:git:git@github.com:scala/scala3.git</connection>
    </scm>
    <developers>
        <developer>
            <id>scala</id>
            <name>The Scala Team</name>
            <url>https://scala-lang.org/</url>
            <email>security@scala-lang.org</email>
        </developer>
    </developers>
    <properties>
        <info.scala.versionLine>Next</info.scala.versionLine>
        <info.versionScheme>semver-spec</info.versionScheme>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.scala-lang</groupId>
            <artifactId>scala3-interfaces</artifactId>
            <version>3.8.0</version>
        </dependency>
        <dependency>
            <groupId>org.scala-lang</groupId>
            <artifactId>tasty-core_3</artifactId>
            <version>3.8.0</version>
        </dependency>
        <dependency>
            <groupId>org.scala-lang</groupId>
            <artifactId>scala3-library_3</artifactId>
            <version>3.8.0</version>
        </dependency>
        <dependency>
            <groupId>org.scala-lang.modules</groupId>
            <artifactId>scala-asm</artifactId>
            <version>9.9.0-scala-1</version>
        </dependency>
        <dependency>
            <groupId>org.scala-sbt</groupId>
            <artifactId>compiler-interface</artifactId>
            <version>1.10.7</version>
        </dependency>
        <dependency>
            <groupId>com.github.sbt</groupId>
            <artifactId>junit-interface</artifactId>
            <version>0.13.3</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>io.get-coursier</groupId>
            <artifactId>coursier_2.13</artifactId>
            <version>2.0.16</version>
            <scope>test</scope>
            <exclusions>
                <exclusion>
                    <groupId>org.scala-lang</groupId>
                    <artifactId>scala-reflect</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
    </dependencies>
    <repositories>
        <repository>
            <id>my-maven-proxy-releases</id>
            <name>my-maven-proxy-releases</name>
            <url>https://scala-webapps.epfl.ch/artifactory/central/</url>
            <layout>default</layout>
        </repository>
        <repository>
            <id>Artifactory</id>
            <name>Artifactory</name>
            <url>https://repo.scala-lang.org/artifactory/fat-jar/</url>
            <layout>default</layout>
        </repository>
    </repositories>
</project>