{
    !! Test unanchored backward seek in branes
    !! Unanchored seek syntax: #-1, #-2, etc.
    !! Only negative offsets are allowed (excluding 0)

    !! Basic unanchored seek
    a = 1;
    b = 2;
    c = #-1 + #-2  !! c = 2 + 1 = 3
}

{
    !! Nested branes with unanchored seek
    x = 10;
    inner = {
        y = 5;
        z = #-1    !! z = 5 (refers to y in same brane)
    };
    result = inner$
}

{
    !! Out of bounds - should be CONSTANIC
    first = 100;
    oob = #-5      !! Out of bounds - becomes CONSTANIC (⎵⎵)
}

{
    !! Complex expression with multiple seeks
    val1 = 3;
    val2 = 4;
    val3 = 5;
    sum = #-1 + #-2 + #-3  !! sum = 5 + 4 + 3 = 12
}

{
    !! User's example: nested assignment with seek
    b = {
        a = 1;
        b = 2;
        c = #-1 + #-2  !! c = 2 + 1 = 3
    }$
}
