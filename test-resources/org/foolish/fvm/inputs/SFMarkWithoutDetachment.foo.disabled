{
  test_1_SFMarked_foolish={
    hello = 5;
    f = <{ r=hello*2; }>
    hello = 6;
    r =$ f;
    !! confirm= r == 12;
  };

  test_2_SFMarked_identifier={
    f = { r=hello*2; }
    hello = 5;
    g = <f>;
    hello = 6;
    r =$ g;
    !! confirm= r == 12;
  };

  test_2_SFMarked_subbrane={
    f = { r=hello*2; }
    hello = 5;
    g = {r=<f>}; !! coordinating to g means f is resolved NOW
    hello = 6;
    r =$ g;
    !! confirm= r == 10;
  };

  test_2_SFMarked_subbrane={
    f = { r=hello*2; }
    hello = 5;

    g0 = {f=1;r=<f>};
    !! confirm g0.r == 1; !! Sanity check this one

    g = <{f=1;r=<f>}>;  !! g.r is kept unresolved by outer SFMark

    hello = 6;
    r =$ g.r;         !! g.r looks for f, finds outter f, looks for hello finds 6
    !! confirm= r == 12;
  };
  
}
