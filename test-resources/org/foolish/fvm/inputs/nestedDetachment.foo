{
    x_outer = 99;
    y = 88;

    outer = {
        a = 1;
        b = 2;
        c = 3;

        !! 1. Redundant blocking [a, a]
        redundant = [a, a] {
            res_a = a;
            res_b = b;
        };

        !! 2. Nested blocking [a][b]
        nested = [a][b] {
            res_a = a;
            res_b = b;
            res_c = c;
        };

        !! 3. Nested blocking same var [a][a]
        nested_redundant = [a][a] {
            res_a = a;
            res_b = b;
        };

        !! 4. Interleaved branes and detachments
        interleaved = {loc_a=10} [b] {
             val_a = loc_a;
             val_b = b;
        };

        !! 5. Regex blocking
        regex_block = [x_*] {
             x_1 = 1;
             res_x1 = x_1; !! Local should be fine
             res_x_outer = x_outer; !! Blocked (matches x_*)
             res_y = y; !! Not blocked
        };

        !! 6. P-Brane (Undetachment)
        pbrane_allow = [a, b] [+a] {
             val_a = a; !! Blocked (Left overrides Right)
             val_b = b; !! Blocked
        };

        pbrane_override = [+a] [a] {
             val_a = a; !! Allowed (Left overrides Right)
        };
    };
}
