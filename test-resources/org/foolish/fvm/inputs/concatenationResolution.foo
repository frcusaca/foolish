{
   test_1_foolish_concatenates_before_resolution = {
      OB={
            a=2;
            r = {a=1}{b=3}{c=a};
      };
      !! confirm = OB$.c == 1
   }


   test_2_serach_concatenates_after_constanicity = {
      OB={
            a=2;
            A={a=1};
            AA={a=parameter};
            B={b=3};
            C={c=AA.parameter};   !! This finds AA, becomes constanic before concatenation and coordination
            f = A AA B C;         !! Still constanic

            parameter = 5;
            r1 = f;

            !! Just paranoid, checking if this comes through the same way?
            r2 = A AA B C;
      }
      !! confirm = OB.r1.c == 5
      !! confirm = OB.r2..c == 5
   }

   test_3_constanic_search_result_concatenates_before_coordination={
      OB={
            A={a=1};
            B={b=3};
            C={c=a};   !! This reaches Constantic before concatenation
            a=-1;
            r=A B C
      }
      !! confirm = OB$.c == 1
   }

   test_4_constant_search_result_stays_constant_during_concatenation={
      OB={
            A={a=1};
            B={b=A.a}; !! This reaches Constant before concatenation (and subsequent coordination)
            C={c=b};   !! This reaches Constantic before concatenation
            b=-2;
            r=A B C
      }
      !! confirm result = OB$.c == 1
   }
}
